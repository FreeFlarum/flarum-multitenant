{
    "name": "manaszon/flarum-ext-hungarian",
    "type": "flarum-extension",
    "description": "Hungarian language pack for Flarum.",
    "homepage": "https://github.com/manaszon/flarum-ext-hungarian",
    "keywords": ["locale", "flarum", "hungarian"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kiss Attila",
            "email": "kissattila97@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/manaszon/flarum-ext-hungarian/issues",
        "source": "https://github.com/manaszon/flarum-ext-hungarian"
