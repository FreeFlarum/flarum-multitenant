{
	"name": "flarum-lang/tamil",
	"type": "flarum-extension",
	"description": "Tamil language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/tamil",
	"keywords": [
		"locale",
		"flarum",
		"flarum-tamil"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Arun Kumar",
			"homepage": "https://discuss.flarum.org/d/27775-tamil-language-pack"
		}
	],
	"support": {
		"issues": "https://github.com/flarum-lang/tamil/issues",
		"source": "https://github.com/flarum-lang/tamil/"
	},
	"require": {
		"flarum/core": "^1.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "0.5.x-dev"
		},
		"flarum-extension": {
			"title": "Tamil",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "ta",
			"title": "தமிழ்"
		}
	}
}
