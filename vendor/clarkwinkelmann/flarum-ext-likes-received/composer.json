{
    "name": "clarkwinkelmann/flarum-ext-likes-received",
    "description": "Shows the total number of likes received on a user profile",
    "keywords": [
        "flarum",
        "likes"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": ">=0.1.0-beta.16 <0.1.0-beta.17",
        "flarum/likes": "*"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "homepage": "https://clarkwinkelmann.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/clarkwinkelmann/flarum-ext-likes-received",
        "forum": "https://discuss.flarum.org/d/24489"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\LikesReceived\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Likes Received",
            "category": "feature",
            "icon": {
                "name": "fas fa-thumbs-up",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/24489"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
