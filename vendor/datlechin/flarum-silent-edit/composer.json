{
    "name": "datlechin/flarum-silent-edit",
    "description": "Allows user groups to edit posts silently",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2"
    },
    "authors": [
        {
            "name": "Ngo Quoc Dat",
            "email": "datlechin@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/datlechin/flarum-silent-edit",
        "forum": "https://discuss.flarum.org/d/30064"
    },
    "autoload": {
        "psr-4": {
            "Datlechin\\SilentEdit\\": "src/"
        }
    },
    "homepage": "https://ngoquocdat.com",
    "extra": {
        "flarum-extension": {
            "title": "Silent Edit",
            "category": "feature",
            "icon": {
                "name": "fas fa-volume-mute",
                "color": "#fff",
                "backgroundColor": "#D3241C"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": true,
                "forum": true,
                "js": true,
                "jsCommon": false,
                "css": false,
                "locale": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": false,
                "bundlewatch": false,
                "backendTesting": false,
                "editorConfig": true,
                "styleci": true
            }
        }
    }
}
