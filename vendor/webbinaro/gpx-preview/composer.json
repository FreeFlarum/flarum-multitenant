{
  "name": "webbinaro/gpx-preview",
  "description": "Adds a live map showing uploaded GPX routes",
  "keywords": [
    "flarum",
    "gpx",
    "gps",
    "map"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^1.0.0",
    "fof/upload": "^1.0.0"
  },
  "authors": [
    {
      "name": "Eddie Webbinaro",
      "email": "eddie@webbinar.dev",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {

      "Webbinaro\\GpxPreview\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "GPX Preview",
      "icon": {
        "name": "fas fa-route",
        "backgroundColor": "#4C804D",
        "color": "#fff"
      }
    }
  }
}
